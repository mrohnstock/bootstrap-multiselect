    "extra": {
        "component": {
            "scripts": [
                "js/bootstrap-multiselect.js",
                "js/bootstrap-multiselect-collapsible-groups.js"
            ],
            "files": [
                "css/bootstrap-multiselect.css",
                "js/bootstrap-multiselect.js",
                "js/bootstrap-multiselect-collapsible-groups.js",
                "less/bootstrap-multiselect.less"
            ],
            "shim": {
                "deps": ["jquery"]
            }
        }
    }
}